/* ==============================
	공지사항
============================== */
.notice-wrap {}
	.notice-wrap .view-move {margin-top:20px;}
	.notice-wrap .btn-place {margin-top:20px; text-align:center;}



/* ==============================
	상세페이지
============================== */
.detail-page {width:1133px !important;}
	.detail-page .btn-top-cover {display:none; position:fixed; width:1131px; margin:auto; bottom:0px; height:0px;}
	.detail-page .btn-top {position:absolute;  right:-35px; bottom:0; width:33px; height:19px;   background:url(../img/btn/btn_top.gif) no-repeat; text-indent:-9999px;}

.goods-spec-wrap {margin-top:5px;}
	.goods-spec-wrap:after, 
	.thumb-area:after {display:block; clear:both; content:"";}

	.goods-spec-wrap .inner-thumb-view {float:left; width:594px;}
	.goods-spec-wrap .inner-spec-view {float:right; width:505px;}

	.inner-thumb-view .big-thumb {float:left; width:500px;}
	.inner-thumb-view .big-thumb img {width:500px; height:500px;}
	.inner-thumb-view .thumb-list-wrap {float:right; margin-top:20px; width:87px;  position:relative;}
	.inner-thumb-view .thumb-list-wrap img {_max-width:87px; _max-height:87px; height:87px; width:87px;}

	.thumb-list-wrap .thumb-list li {margin-bottom:6px;}
	.thumb-list-wrap .thumb-list li a {display:block; position:relative;}
	/* .thumb-list-wrap .bx-wrapper {margin-top:20px !important;} */

	.goods-spec-wrap .inner-spec-view .opt-choice div.select {width:260px;}

/* 상품상세 썸네일 */
.thumb-list-wrap .bx-pager {display:none;}
	.thumb-list-wrap .bx-controls-direction .bx-prev,
	.thumb-list-wrap .bx-controls-direction .bx-next {display:block; width:37px; height:13px; background:url(../img/btn/rolling_arrow_vertical.gif) no-repeat; text-indent:-9999px;}
	.thumb-list-wrap .bx-controls-direction .bx-prev {position:absolute; top:-20px;  left:25px; background-position:0 -13px;}
	.thumb-list-wrap .bx-controls-direction .bx-prev:hover {background-position:0 0;}
	.thumb-list-wrap .bx-controls-direction .bx-next {margin:5px 0 0 25px; background-position:-37px -13px;}
	.thumb-list-wrap .bx-controls-direction .bx-next:hover {background-position:-37px 0;}

.thumb-list-wrap .thumb-list li a.on:after,
	.thumb-list-wrap .thumb-list li a:after {display:none; position:absolute; top:0px; left:0px; width:100%; height:100%; border:1px solid #838383; box-sizing:border-box; content:"";}
	.thumb-list-wrap .thumb-list li a.on:after,
	.thumb-list-wrap .thumb-list li a:hover:after {display:block;}

/* 프로모션 & SHARE */
.promotion-sns {margin-top:15px; width:500px; text-align:center;}
	.promotion-sns dt {color:#4b4b4b; padding-bottom:5px; font-family:ls;}

	.promotion-sns .promotion {display:inline-block; width:315px; vertical-align:top;}
	.promotion-sns .promotion dd {text-align:left; padding-left:30px;}
	.promotion-sns .promotion dd a {display:block; color:#848484; line-height:1.4; text-overflow:ellipsis; overflow:hidden; white-space:nowrap;}

	.promotion-sns .sns-share {display:inline-block; width:180px; text-align:center; vertical-align:top;}
	.promotion-sns .sns-share dd {display:inline-block; width:30px; margin:0 1px;}
	.promotion-sns .sns-share a {display:block;width:30px; height:30px; background:url(../img/icon/social_icon02.png) no-repeat; text-align:left; text-indent:-9999px;}

	.promotion-sns .sns-share a.kas {background-position:0 0;}
	.promotion-sns .sns-share a.kas:hover {background-position:-30px 0;}
	.promotion-sns .sns-share a.facebook {background-position:0 -30px;}
	.promotion-sns .sns-share a.facebook:hover {background-position:-30px -30px;}
	.promotion-sns .sns-share a.twitter {background-position:0 -60px;}
	.promotion-sns .sns-share a.twitter:hover {background-position:-30px -60px;}


/* 상품별 아이콘 */
.text-icon {text-align:center; margin-bottom:10px;}
	.text-icon span {display:inline-block; padding:0 3px; min-width:46px; margin-left:-3px; color:#fff; font-size:10px; box-sizing:border-box; text-transform:uppercase;}
	.text-icon span.sale {background:#c85f75;}
	.text-icon span.best {background:#93617e;}
	.text-icon span.special {background:#528f93;}
	.text-icon span.only {background:#4b4b4b;}

/* 상품이름 */
.product-name {padding:0 50px; text-align:center; color:#5a5959; font-size:17px; font-weight:normal;}
	.product-name .wish-icon {display:inline-block; margin-top:-2px; width:18px; height:18px; background:url(../img/icon/wish_star.gif) no-repeat; vertical-align:middle; text-indent:-9999px;}
	.product-name .wish-icon.on {background-position:0 -18px;}

/* 상품가격 */
.product-price {margin-top:5px; margin-bottom:10px;text-align:center; position:relative;}
	.product-price del {display:block; color:#a9a9a9; font-size:18px;}
	.product-price strong {display:block; color:#4b4b4b; font-size:31px; font-weight:normal; font-family:ngeb;}
	.product-price .dc-per {display:block; position:absolute; padding:5px 10px; right:11px; bottom:0px; background:#4b4b4b; color:#fff; font-size:22px; font-family:ngb;}
	.product-price .dc-per:after {display:inline-block;  font-size:12px; vertical-align:middle; content:"▼";}

/* 마일리지 정보 */
.mileage-info {_margin-top:10px; padding:5px 0;border-top:1px solid #d8d8d8; font-family:ng;}
	.mileage-info li {padding:5px 0;}
	.mileage-info li span {display:inline-block; color:#8b8b8b; padding-left:10px; width:165px;}
	.mileage-info li span:after {display:inline-block; margin:0 8px 0 77px; width:16px; height:15px; background:#3c3c3c; border-radius:2px; color:#dcdcdc; font-size:11px; text-align:center; content:"M";}

/* 옵션선택 */
.opt-choice {padding:5px 0; border-top:1px solid #d8d8d8; }
