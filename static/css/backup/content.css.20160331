	.detail-buy-btn .buy-btn a.cart:before {display:inline-block; margin:-3px 3px 0 0; width:19px; height:18px; background:url(../img/icon/icon_btn_wish.png) no-repeat; vertical-align:middle; content:"";}
	.detail-buy-btn .buy-btn a.wish:before {display:inline-block; margin:-3px 3px 0 0; width:19px; height:18px; background:url(../img/icon/icon_btn_cart.png) no-repeat; vertical-align:middle; content:"";}

	.detail-buy-btn .cash-shop {display:inline-block; position:absolute; top:0px; right:10px; width:200px; height:68px; border:1px solid #c8c8c8; background:url(../img/icon/btn_cash_shop.jpg) center no-repeat; text-align:center;  color:#4b4b4b; box-sizing:border-box;}
	.detail-buy-btn .cash-shop span {display:block; padding:12px 0 10px 0; font-size:17px; font-family:"ls","ng"}
